        <Label layoutX="13.0" layoutY="149.0" text="Address" />
        <Label layoutX="11.0" layoutY="196.0" text="Zip Code" />
        <Label layoutX="14.0" layoutY="243.0" text="Contact" />
        <Label layoutX="14.0" layoutY="285.0" text="Country" />
        <Label layoutX="14.0" layoutY="327.0" text="Division" />
        <TextField fx:id="customerIDField" editable="false" layoutX="130.0" layoutY="57.0" prefHeight="26.0" prefWidth="178.0" promptText="Auto-Generated" />
        <TextField fx:id="customerNameField" layoutX="130.0" layoutY="101.0" prefHeight="26.0" prefWidth="178.0" />
        <TextField fx:id="customerAddressField" layoutX="130.0" layoutY="145.0" prefHeight="26.0" prefWidth="178.0" />
