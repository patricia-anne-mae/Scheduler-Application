<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="355.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.loginViewController">
    <children>
        <Label fx:id="logIn" layoutX="136.0" layoutY="14.0" text="Log In">
            <font>
                <Font name="System Bold" size="25.0" />
            </font>
        </Label>
        <Label fx:id="usernameLogin" layoutX="98.0" layoutY="94.0" text="Username" />
        <TextField fx:id="usernameField" layoutX="98.0" layoutY="111.0" />
        <Label fx:id="passwordLogin" layoutX="99.0" layoutY="156.0" text="Password" />
        <TextField fx:id="passwordField" layoutX="98.0" layoutY="173.0" />
        <Button fx:id="loginButton" layoutX="152.0" layoutY="215.0" mnemonicParsing="false" onAction="#onActionLogin" text="Log In" />
        <Label fx:id="locationLabel" layoutX="23.0" layoutY="343.0" text="Location:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <Label fx:id="locationZone" layoutX="105.0" layoutY="343.0" text="Zone" />
    </children>
</AnchorPane>
